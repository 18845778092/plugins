{
  "name": "farm-plugin-react-components",
  "version": "0.0.1",
  "main": "scripts/index.js",
  "types": "scripts/index.d.ts",
  "type": "module",
  "license": "MIT",
  "napi": {
    "name": "farm_plugin_react_components",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "x86_64-unknown-linux-musl",
        "i686-pc-windows-msvc",
        "aarch64-pc-windows-msvc"
      ]
    }
  },
  "exports": {
    ".": {
      "import": "./scripts/index.js",
      "require": "./scripts/index.js",
      "types": "./scripts/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "cargo watch -w src -s 'scripts/watch.sh'",
    "bump": "npx changeset version",
    "play": "cd playground && farm",
    "build": "farm-plugin-tools build --platform --cargo-name farm_plugin_react_components -p farm_plugin_react_components --release",
    "prepublishOnly": "farm-plugin-tools prepublish",
    "publish": "npm publish"
  },
  "files": [
    "scripts"
  ]
}
